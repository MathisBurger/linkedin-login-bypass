{
  "manifest_version": 3,
  "name": "LinkedIn No Login",
  "description": "Simple plugin to bypass linkedIn login",
  "version": "1.0",
  "action": {
    "default_popup": "default.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://www.linkedin.com/in/*",
        "https://de.linkedin.com/in/*"
      ]
    }
  ]
}
